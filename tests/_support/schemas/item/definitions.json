{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "item": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "listId": {
          "type": "string"
        },
        "secret": {
          "type": "string"
        },
        "meta": {
          "type": "object",
          "properties": {
            "attachCount": {
              "type": "integer"
            },
            "webSite": {
              "type": ["null", "string"]
            }
          },
          "required": [
            "attachCount",
            "webSite"
          ]
        },
        "invited": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "userId": {
                "type": "string"
              },
              "userAvatar": {
                "type": ["string", "null"]
              },
              "userName": {
                "type": "string"
              },
              "userEmail": {
                "type": "string"
              },
              "userDomainRoles": {
                "type": "array"
              },
              "access": {
                "type": "string"
              }
            }
          }
        },
        "shared": {
          "type": ["object", "null"],
          "properties": {
            "id": {
              "type": "string"
            },
            "teamId": {
              "type": ["string", "null"]
            },
            "userId": {
              "type": "string"
            },
            "email": {
              "type": "string"
            },
            "lastUpdated": {
              "type": "string"
            },
            "access": {
              "type": "string"
            },
            "link": {
              "type": ["string", "null"]
            },
            "isAccepted": {
              "type": "boolean"
            },
            "publicKey": {
              "type": ["string", "null"]
            }
          }
        },
        "lastUpdated": {
          "type": "string"
        },
        "tags": {
          "type": ["array", "null"],
          "items": {
            "type": "string"
          }
        },
        "favorite": {
          "type": "boolean"
        },
        "ownerId": {
          "type": "string"
        },
        "originalItemId": {
          "type": ["string", "null"]
        },
        "previousListId": {
          "type": ["string", "null"]
        },
        "isShared": {
          "type": "boolean"
        } ,
        "title": {
          "type": ["string", "null"]
        }
      },
      "required": [
        "id",
        "type",
        "listId",
        "title",
        "secret",
        "invited",
        "shared",
        "isShared",
        "lastUpdated",
        "tags",
        "favorite",
        "ownerId",
        "originalItemId",
        "previousListId"
      ]
    },
    "list": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "sort": {
          "type": "integer"
        },
        "children": {
          "type": "array"
        },
        "label": {
          "type": "string"
        },
        "teamId": {
          "type": ["string", "null"]
        }
      },
      "required": [
        "id",
        "type",
        "sort",
        "children",
        "label",
        "teamId"
      ]
    },
    "share": {
      "type": "object",
      "properties": {
        "userId": {
          "type": "string"
        },
        "keypairId": {
          "type": "string"
        }
      },
      "required": [
        "userId",
        "keypairId"
      ]
    }
  }
}