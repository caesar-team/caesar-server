{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-bcmath": "*",
        "ext-iconv": "*",
        "ext-redis": "*",
        "caesar/billing-subscription": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "3.1.0",
        "easycorp/easyadmin-bundle": "2.1.1",
        "fresh/doctrine-enum-bundle": "6.3.0",
        "friendsofsymfony/rest-bundle": "2.5.0",
        "friendsofsymfony/user-bundle": "2.1.2",
        "guzzlehttp/guzzle": "6.3.3",
        "hashids/hashids": "3.0.0",
        "hwi/oauth-bundle": "0.6.3",
        "lexik/jwt-authentication-bundle": "2.6.3",
        "nelmio/api-doc-bundle": "3.4.0",
        "nelmio/cors-bundle": "1.5.5",
        "pear/math_biginteger": "^1.0",
        "php-amqplib/rabbitmq-bundle": "1.14.4",
        "php-http/guzzle6-adapter": "1.1.1",
        "php-http/httplug-bundle": "1.15.2",
        "predis/predis": "1.1.1",
        "psr/simple-cache": "1.0.1",
        "ramsey/uuid": "3.8.0",
        "ramsey/uuid-doctrine": "1.5.0",
        "scheb/two-factor-bundle": "3.18.0",
        "sensio/framework-extra-bundle": "5.3.1",
        "sensiolabs/security-checker": "5.0.3",
        "snc/redis-bundle": "2.1.9",
        "stof/doctrine-extensions-bundle": "1.3.0",
        "sylius/mailer-bundle": "1.4.3",
        "symfony/asset": "^4.3",
        "symfony/console": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/expression-language": "^4.3",
        "symfony/flex": "1.2.3",
        "symfony/framework-bundle": "^4.3",
        "symfony/http-foundation": "^4.3",
        "symfony/monolog-bundle": "3.3.1",
        "symfony/orm-pack": "1.0.6",
        "symfony/serializer-pack": "1.0.2",
        "symfony/swiftmailer-bundle": "3.2.6",
        "symfony/twig-bundle": "^4.3",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.15.1",
        "phpunit/phpunit": "8.3.4",
        "symfony/browser-kit": "4.3.3",
        "symfony/debug-pack": "1.0.7",
        "symfony/maker-bundle": "1.12.0",
        "symfony/phpunit-bridge": "4.3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security:check": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "",
            "allow-contrib": false
        }
    }
}
